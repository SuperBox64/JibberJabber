{
  "name": "Objective-C++",
  "ext": ".mm",
  "comment": "//",
  "intFmt": "%d",
  "strFmt": "%s",
  "doubleFmt": "%g",
  "boolFmt": "%s",
  "coutExpr": "std::cout << {expr}",
  "coutEndl": " << std::endl;",
  "coutSep": " << ",
  "coutNewline": "std::cout << {expr} << std::endl;",
  "coutInline": "std::cout << {expr};",
  "arrayLitOpen": "@[",
  "arrayLitClose": "]",
  "dictLitOpen": "@{",
  "dictLitClose": "}",
  "boxString": "@{expr}",
  "boxValue": "@({expr})",
  "arrayType": "NSArray *",
  "loopIndexType": "NSUInteger",
  "strSelector": "UTF8String",
  "doubleSelector": "doubleValue",
  "intSelector": "intValue",
  "selectorAccess": "[{expr} {selector}]",
  "enumAccess": "{key}",
  "header": "// Transpiled from JibJab\n#import <Foundation/Foundation.h>\n#include <iostream>\n#include <cstdint>\n",
  "printInt": "std::cout << {expr} << std::endl;",
  "printStr": "std::cout << {expr} << std::endl;",
  "printFloat": "std::cout << {expr} << std::endl;",
  "printDouble": "std::cout << {expr} << std::endl;",
  "printBool": "std::cout << ({expr} ? \"true\" : \"false\") << std::endl;",
  "var": "{type} {name} = {value};",
  "varAuto": "auto {name} = {value};",
  "forRange": "for (int {var} = {start}; {var} < {end}; {var}++) {",
  "while": "while ({condition}) {",
  "if": "if ({condition}) {",
  "else": "} else {",
  "try": "@try {",
  "catch": "} @catch (NSException *e) {",
  "catchVar": "} @catch (NSException *_e) {",
  "catchVarBind": "NSString *{var} = [_e reason];",
  "func": "{type} {name}({params}) {",
  "funcDecl": "{type} {name}({params});",
  "return": "return {value};",
  "throw": "@throw [NSException exceptionWithName:@\"JJError\" reason:@{value} userInfo:nil];",
  "call": "{name}({args})",
  "blockEnd": "}",
  "indent": "    ",
  "true": "true",
  "false": "false",
  "nil": "nil",
  "main": "int main(int argc, const char * argv[]) {\n    @autoreleasepool {\n{body}    }\n    return 0;\n}",
  "stringType": "std::string",
  "floatMod": "fmod({left}, {right})",
  "enum": "enum {name} { {cases} };",
  "collectionStyle": "expand",
  "expandBoolAsInt": true,
  "expandStringType": "const char*",
  "compile": ["clang++", "-framework", "Foundation", "{src}", "-o", "{out}"],
  "types": {
    "Int": "int",
    "Int8": "int8_t",
    "Int16": "int16_t",
    "Int32": "int32_t",
    "Int64": "int64_t",
    "UInt": "unsigned int",
    "UInt8": "uint8_t",
    "UInt16": "uint16_t",
    "UInt32": "uint32_t",
    "UInt64": "uint64_t",
    "Float": "float",
    "Double": "double"
  },
  "highlightKeywords": ["break", "case", "catch", "continue", "default", "delete", "do", "else", "for", "goto", "if", "new", "noexcept", "return", "sizeof", "switch", "throw", "try", "while", "super"],
  "highlightDeclKeywords": ["auto", "class", "const", "constexpr", "enum", "explicit", "export", "extern", "final", "friend", "inline", "mutable", "namespace", "operator", "override", "private", "protected", "public", "register", "static", "struct", "template", "typedef", "typename", "union", "using", "virtual", "volatile", "nil", "Nil", "YES", "NO", "NULL", "nullptr"],
  "highlightTypeKeywords": ["int", "char", "void", "float", "double", "short", "long", "unsigned", "signed", "bool", "string", "vector", "id", "BOOL", "NSString", "NSArray", "NSDictionary", "NSNumber", "NSObject", "true", "false"],
  "highlightSelfKeywords": ["self", "this"],
  "highlightSystemFunctions": ["printf", "NSLog", "fprintf", "sprintf", "scanf", "puts", "malloc", "calloc", "realloc", "free", "memcpy", "memmove", "memset", "strlen", "strcmp", "sort", "find", "push_back", "begin", "end", "make_shared", "make_unique", "move", "swap", "exit", "abort", "abs"],
  "highlightCommentPrefix": "//"
}
