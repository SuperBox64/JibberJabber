{
  "name": "JavaScript",
  "ext": ".js",
  "comment": "//",
  "interpOpen": "`",
  "interpClose": "`",
  "interpVarOpen": "${",
  "interpVarClose": "}",
  "enumAccess": "{name}[{key}]",
  "header": "// Transpiled from JibJab\n",
  "print": "print({expr});",
  "printBool": "print({expr});",
  "log": "console.log({expr});",
  "logBool": "console.log({expr});",
  "var": "let {name} = {value};",
  "const": "const {name} = {value};",
  "forRange": "for (let {var} = {start}; {var} < {end}; {var}++) {",
  "forIn": "for (const {var} of {collection}) {",
  "while": "while ({condition}) {",
  "if": "if ({condition}) {",
  "else": "} else {",
  "try": "try {",
  "catch": "} catch (e) {",
  "catchVar": "} catch ({var}) {",
  "func": "function {name}({params}) {",
  "return": "return {value};",
  "throw": "throw {value};",
  "call": "{name}({args})",
  "random": "Math.floor(Math.random() * ({max} - {min} + 1)) + {min}",
  "blockEnd": "}",
  "indent": "    ",
  "true": "true",
  "false": "false",
  "nil": "null",
  "eq": "===",
  "neq": "!==",
  "enum": "const {name} = { {cases} };",
  "collectionStyle": "native",
  "compile": ["qjsc", "-o", "{out}", "{src}"],
  "run": ["qjs", "{src}"],
  "types": {
    "Int": "Number",
    "Int8": "Number",
    "Int16": "Number",
    "Int32": "Number",
    "Int64": "BigInt",
    "UInt": "Number",
    "UInt8": "Number",
    "UInt16": "Number",
    "UInt32": "Number",
    "UInt64": "BigInt",
    "Float": "Number",
    "Double": "Number"
  },
  "highlightKeywords": ["if", "else", "for", "while", "do", "return", "break", "continue", "switch", "case", "default", "try", "catch", "finally", "throw", "new", "async", "await", "typeof", "instanceof", "in", "of", "delete", "void", "yield", "super"],
  "highlightDeclKeywords": ["function", "var", "let", "const", "class", "extends", "static", "import", "export", "from"],
  "highlightTypeKeywords": ["true", "false", "null", "undefined", "NaN", "Infinity", "Array", "Object", "String", "Number", "Boolean", "Map", "Set", "Promise", "Date", "RegExp", "Error", "Symbol", "BigInt", "JSON", "Math", "console"],
  "highlightSelfKeywords": ["this"],
  "highlightSystemFunctions": ["alert", "confirm", "prompt", "parseInt", "parseFloat", "isNaN", "isFinite", "decodeURI", "encodeURI", "decodeURIComponent", "encodeURIComponent", "setTimeout", "setInterval", "clearTimeout", "clearInterval", "fetch", "require"],
  "highlightCommentPrefix": "//"
}
