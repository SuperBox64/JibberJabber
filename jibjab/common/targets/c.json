{
  "name": "C",
  "ext": ".c",
  "comment": "//",
  "intFmt": "%d",
  "strFmt": "%s",
  "doubleFmt": "%g",
  "boolFmt": "%s",
  "printfInterp": "printf(\"{fmt}\\n\"{args});",
  "printfInline": "printf(\"{fmt}\"{args});",
  "enumAccess": "{key}",
  "header": "// Transpiled from JibJab\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdint.h>\n#include <math.h>\n",
  "printInt": "printf(\"%d\\n\", {expr});",
  "printStr": "printf(\"%s\\n\", {expr});",
  "printFloat": "printf(\"%f\\n\", {expr});",
  "printBool": "printf(\"%s\\n\", {expr} ? \"true\" : \"false\");",
  "printDouble": "printf(\"%lf\\n\", {expr});",
  "var": "{type} {name} = {value};",
  "forRange": "for (int {var} = {start}; {var} < {end}; {var}++) {",
  "while": "while ({condition}) {",
  "if": "if ({condition}) {",
  "else": "} else {",
  "try": "if (1) {",
  "catch": "} if (0) {",
  "func": "{type} {name}({params}) {",
  "funcDecl": "{type} {name}({params});",
  "return": "return {value};",
  "throw": "/* throw */ abort();",
  "call": "{name}({args})",
  "blockEnd": "}",
  "indent": "    ",
  "true": "1",
  "false": "0",
  "nil": "0",
  "main": "int main() {\n{body}    return 0;\n}",
  "stringType": "const char*",
  "floatMod": "fmod({left}, {right})",
  "enum": "enum {name} { {cases} };",
  "collectionStyle": "expand",
  "expandBoolAsInt": true,
  "compile": ["clang", "{src}", "-o", "{out}"],
  "types": {
    "Int": "int",
    "Int8": "int8_t",
    "Int16": "int16_t",
    "Int32": "int32_t",
    "Int64": "int64_t",
    "UInt": "unsigned int",
    "UInt8": "uint8_t",
    "UInt16": "uint16_t",
    "UInt32": "uint32_t",
    "UInt64": "uint64_t",
    "Float": "float",
    "Double": "double"
  },
  "highlightKeywords": ["break", "case", "continue", "default", "do", "else", "for", "goto", "if", "return", "sizeof", "switch", "while"],
  "highlightDeclKeywords": ["auto", "const", "enum", "extern", "inline", "register", "static", "struct", "typedef", "union", "volatile"],
  "highlightTypeKeywords": ["int", "char", "void", "float", "double", "short", "long", "unsigned", "signed", "size_t", "bool", "FILE", "NULL", "true", "false"],
  "highlightSystemFunctions": ["printf", "fprintf", "sprintf", "snprintf", "scanf", "fscanf", "sscanf", "puts", "fputs", "gets", "fgets", "putchar", "getchar", "malloc", "calloc", "realloc", "free", "memcpy", "memmove", "memset", "memcmp", "strlen", "strcpy", "strncpy", "strcat", "strncat", "strcmp", "strncmp", "fopen", "fclose", "fread", "fwrite", "fseek", "ftell", "exit", "abort", "atoi", "atof", "atol", "abs", "rand", "srand"],
  "highlightCommentPrefix": "//"
}
