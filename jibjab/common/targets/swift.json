{
  "name": "Swift",
  "ext": ".swift",
  "comment": "//",
  "interpOpen": "\"",
  "interpClose": "\"",
  "interpVarOpen": "\\(",
  "interpVarClose": ")",
  "enumAccess": "{name}.{key}",
  "enumSelf": "{name}.self",
  "varDict": "var {name}: [String: Any] = {value}",
  "dictEmpty": "[:]",
  "paramFormat": "_ {name}: {type}",
  "header": "// Transpiled from JibJab\nimport Foundation\nfunc _input(_ p: String) -> String { print(\"__JJPROMPT__:\\(p)\"); let s = readLine() ?? \"\"; print(\"\\(p)\\(s)\"); return s }\n",
  "logImport": "import os.log",
  "errorStruct": "struct JJError: Error { let message: String }",
  "print": "print({expr})",
  "printBool": "print({expr})",
  "log": "os_log(\"%{public}@\", \"\\({expr})\")",
  "logBool": "os_log(\"%{public}@\", \"\\({expr})\")",
  "var": "var {name}: {type} = {value}",
  "varInfer": "var {name} = {value}",
  "const": "let {name}: {type} = {value}",
  "constInfer": "let {name} = {value}",
  "forRange": "for {var} in {start}..<{end} {",
  "forIn": "for {var} in {collection} {",
  "while": "while {condition} {",
  "if": "if {condition} {",
  "else": "} else {",
  "try": "do {",
  "catch": "} catch {",
  "func": "func {name}({params}) -> Int {",
  "return": "return {value}",
  "throw": "throw JJError(message: {value})",
  "catchVarBind": "let {var} = (error as? JJError)?.message ?? \"\\(error)\"",
  "call": "{name}({args})",
  "random": "Int.random(in: {min}...{max})",
  "input": "_input({prompt})",
  "blockEnd": "}",
  "indent": "    ",
  "true": "true",
  "false": "false",
  "nil": "nil",
  "floatMod": "{left}.truncatingRemainder(dividingBy: {right})",
  "enum": "enum {name} { case {cases} }",
  "collectionStyle": "native",
  "compile": ["swiftc", "{src}", "-o", "{out}"],
  "types": {
    "Int": "Int",
    "Int8": "Int8",
    "Int16": "Int16",
    "Int32": "Int32",
    "Int64": "Int64",
    "UInt": "UInt",
    "UInt8": "UInt8",
    "UInt16": "UInt16",
    "UInt32": "UInt32",
    "UInt64": "UInt64",
    "Float": "Float",
    "Double": "Double"
  },
  "highlightKeywords": ["if", "else", "guard", "for", "while", "repeat", "switch", "case", "default", "break", "continue", "return", "defer", "do", "try", "catch", "throw", "throws", "rethrows", "where", "as", "is", "in", "super", "async", "await"],
  "highlightDeclKeywords": ["func", "var", "let", "class", "struct", "enum", "extension", "protocol", "mutating", "inout", "import", "public", "private", "internal", "fileprivate", "open", "final", "required", "convenience", "static", "subscript", "init", "deinit", "override", "typealias", "associatedtype", "some", "any", "actor", "nonisolated", "isolated", "sending", "weak", "unowned", "lazy", "dynamic", "indirect", "consuming", "borrowing"],
  "highlightTypeKeywords": ["Int", "Int8", "Int16", "Int32", "Int64", "UInt", "UInt8", "UInt16", "UInt32", "UInt64", "String", "Double", "Float", "Bool", "Character", "Array", "Dictionary", "Set", "Optional", "Any", "AnyObject", "Void", "Error", "Result", "Codable", "Encodable", "Decodable", "Hashable", "Equatable", "Comparable", "Identifiable", "Sendable", "CustomStringConvertible", "true", "false", "nil"],
  "highlightSelfKeywords": ["self", "Self"],
  "highlightSystemFunctions": ["print", "debugPrint", "dump", "fatalError", "preconditionFailure", "precondition", "assert", "assertionFailure", "min", "max", "abs", "stride", "zip", "type", "isKnownUniquelyReferenced", "swap", "withUnsafePointer", "withUnsafeMutablePointer", "withUnsafeBytes", "MemoryLayout", "unsafeBitCast", "numericCast", "readLine", "repeatElement", "sequence"],
  "highlightCommentPrefix": "//"
}
