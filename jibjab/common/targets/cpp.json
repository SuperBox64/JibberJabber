{
  "name": "C++",
  "ext": ".cpp",
  "comment": "//",
  "intFmt": "%d",
  "strFmt": "%s",
  "doubleFmt": "%g",
  "boolFmt": "%s",
  "printfInterp": "printf(\"{fmt}\\n\"{args});",
  "printfInline": "printf(\"{fmt}\"{args});",
  "coutExpr": "std::cout << {expr}",
  "coutEndl": " << std::endl;",
  "coutSep": " << ",
  "coutNewline": "std::cout << {expr} << std::endl;",
  "coutInline": "std::cout << {expr};",
  "enumAccess": "static_cast<int>({name}::{key})",
  "stringInclude": "#include <string>",
  "header": "// Transpiled from JibJab\n#include <iostream>\n#include <cstdint>\n#include <cmath>\n",
  "printInt": "std::cout << {expr} << std::endl;",
  "printStr": "std::cout << {expr} << std::endl;",
  "printFloat": "std::cout << {expr} << std::endl;",
  "printDouble": "std::cout << {expr} << std::endl;",
  "printBool": "std::cout << ({expr} ? \"true\" : \"false\") << std::endl;",
  "logInt": "std::cerr << {expr} << std::endl;",
  "logStr": "std::cerr << {expr} << std::endl;",
  "logFloat": "std::cerr << {expr} << std::endl;",
  "logDouble": "std::cerr << {expr} << std::endl;",
  "logBool": "std::cerr << ({expr} ? \"true\" : \"false\") << std::endl;",
  "cerrExpr": "std::cerr << {expr}",
  "cerrEndl": " << std::endl;",
  "cerrSep": " << ",
  "cerrNewline": "std::cerr << {expr} << std::endl;",
  "cerrInline": "std::cerr << {expr};",
  "var": "{type} {name} = {value};",
  "varAuto": "auto {name} = {value};",
  "forRange": "for (int {var} = {start}; {var} < {end}; {var}++) {",
  "while": "while ({condition}) {",
  "if": "if ({condition}) {",
  "else": "} else {",
  "try": "try {",
  "catch": "} catch (...) {",
  "catchVar": "} catch (const std::exception& _e) {",
  "catchVarBind": "std::string {var} = _e.what();",
  "func": "{type} {name}({params}) {",
  "funcDecl": "{type} {name}({params});",
  "return": "return {value};",
  "throw": "throw std::runtime_error({value});",
  "call": "{name}({args})",
  "blockEnd": "}",
  "indent": "    ",
  "true": "true",
  "false": "false",
  "nil": "0",
  "main": "int main() {\n{body}    return 0;\n}",
  "stringType": "std::string",
  "floatMod": "fmod({left}, {right})",
  "enum": "enum class {name} { {cases} };",
  "collectionStyle": "expand",
  "compile": ["clang++", "{src}", "-o", "{out}"],
  "types": {
    "Int": "int",
    "Int8": "int8_t",
    "Int16": "int16_t",
    "Int32": "int32_t",
    "Int64": "int64_t",
    "UInt": "unsigned int",
    "UInt8": "uint8_t",
    "UInt16": "uint16_t",
    "UInt32": "uint32_t",
    "UInt64": "uint64_t",
    "Float": "float",
    "Double": "double"
  },
  "highlightKeywords": ["break", "case", "catch", "continue", "default", "delete", "do", "else", "for", "goto", "if", "new", "noexcept", "return", "sizeof", "switch", "throw", "try", "while"],
  "highlightDeclKeywords": ["auto", "class", "const", "constexpr", "enum", "explicit", "export", "extern", "final", "friend", "inline", "mutable", "namespace", "operator", "override", "private", "protected", "public", "register", "static", "static_cast", "struct", "template", "typedef", "typeid", "typename", "union", "using", "virtual", "volatile", "nullptr"],
  "highlightTypeKeywords": ["int", "char", "void", "float", "double", "short", "long", "unsigned", "signed", "bool", "string", "vector", "map", "set", "pair", "size_t", "true", "false", "NULL", "cout", "cin", "endl", "cerr"],
  "highlightSelfKeywords": ["this"],
  "highlightSystemFunctions": ["printf", "fprintf", "sprintf", "snprintf", "scanf", "puts", "fputs", "getchar", "putchar", "malloc", "calloc", "realloc", "free", "memcpy", "memmove", "memset", "strlen", "strcmp", "exit", "abort", "abs", "rand", "srand", "sort", "find", "push_back", "emplace_back", "begin", "end", "make_shared", "make_unique", "make_pair", "move", "forward", "swap"],
  "highlightCommentPrefix": "//"
}
