{
  "name": "C++",
  "ext": ".cpp",
  "header": "// Transpiled from JibJab\n#include <iostream>\n#include <cstdint>\n",
  "printInt": "std::cout << {expr} << std::endl;",
  "printStr": "std::cout << {expr} << std::endl;",
  "printFloat": "std::cout << {expr} << std::endl;",
  "printDouble": "std::cout << {expr} << std::endl;",
  "var": "{type} {name} = {value};",
  "varAuto": "auto {name} = {value};",
  "forRange": "for (int {var} = {start}; {var} < {end}; {var}++) {",
  "while": "while ({condition}) {",
  "if": "if ({condition}) {",
  "else": "} else {",
  "func": "{type} {name}({params}) {",
  "funcDecl": "{type} {name}({params});",
  "return": "return {value};",
  "call": "{name}({args})",
  "blockEnd": "}",
  "indent": "    ",
  "true": "true",
  "false": "false",
  "nil": "0",
  "main": "int main() {\n{body}    return 0;\n}",
  "types": {
    "Int": "int",
    "Int8": "int8_t",
    "Int16": "int16_t",
    "Int32": "int32_t",
    "Int64": "int64_t",
    "UInt": "unsigned int",
    "UInt8": "uint8_t",
    "UInt16": "uint16_t",
    "UInt32": "uint32_t",
    "UInt64": "uint64_t",
    "Float": "float",
    "Double": "double"
  }
}
