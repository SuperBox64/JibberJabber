{
  "version": "1.0.0",
  "keywords": {
    "print": "~>frob{7a3}",
    "input": "~>slurp{9f2}",
    "yeet": "~>yeet",
    "snag": "~>snag",
    "invoke": "~>invoke",
    "enum": "~>enum",
    "nil": "~nil",
    "true": "~yep",
    "false": "~nope"
  },
  "blocks": {
    "loop": "<~loop{",
    "when": "<~when{",
    "else": "<~else>>",
    "morph": "<~morph{",
    "try": "<~try>>",
    "oops": "<~oops>>",
    "end": "<~>>"
  },
  "blockSuffix": "}>>",
  "operators": {
    "add": {"symbol": "<+>", "emit": "+"},
    "sub": {"symbol": "<->", "emit": "-"},
    "mul": {"symbol": "<*>", "emit": "*"},
    "div": {"symbol": "</>", "emit": "/"},
    "mod": {"symbol": "<%>", "emit": "%"},
    "eq":  {"symbol": "<=>", "emit": "=="},
    "neq": {"symbol": "<!=>", "emit": "!="},
    "lt":  {"symbol": "<lt>", "emit": "<"},
    "lte": {"symbol": "<lte>", "emit": "<="},
    "gt":  {"symbol": "<gt>", "emit": ">"},
    "gte": {"symbol": "<gte>", "emit": ">="},
    "and": {"symbol": "<&&>", "emit": "&&"},
    "or":  {"symbol": "<||>", "emit": "||"},
    "not": {"symbol": "<!>", "emit": "!"}
  },
  "structure": {
    "action": "::",
    "range": "..",
    "colon": ":"
  },
  "syntax": {
    "emit": "emit",
    "grab": "grab",
    "val": "val",
    "with": "with",
    "cases": "cases"
  },
  "literals": {
    "numberPrefix": "#",
    "stringDelim": "\"",
    "comment": "@@"
  }
}
