      "blockEnd": "}",
      "indent": "    ",
      "true": "true",
      "false": "false",
      "nil": "0",
      "main": "int main() {\n{body}    return 0;\n}"
    },
    "objc": {
      "name": "Objective-C",
      "ext": ".m",
      "header": "// Transpiled from JibJab\n#import <Foundation/Foundation.h>\n",
      "printInt": "NSLog(@\"%d\", {expr});",
