    "objcpp": {
      "name": "Objective-C++",
      "ext": ".mm",
      "header": "// Transpiled from JibJab\n#import <Foundation/Foundation.h>\n#include <iostream>\n",
      "printInt": "std::cout << {expr} << std::endl;",
