{
  "version": "1.0.0",
  "keywords": {
    "print": "~>frob{7a3}",
    "input": "~>slurp{9f2}",
    "yeet": "~>yeet",
    "snag": "~>snag",
    "invoke": "~>invoke",
    "nil": "~nil",
    "true": "~yep",
    "false": "~nope"
  },
  "blocks": {
    "loop": "<~loop{",
    "when": "<~when{",
    "else": "<~else>>",
    "morph": "<~morph{",
    "try": "<~try>>",
    "oops": "<~oops>>",
    "end": "<~>>"
  },
  "blockSuffix": "}>>",
  "operators": {
    "add": {"symbol": "<+>", "emit": "+"},
    "sub": {"symbol": "<->", "emit": "-"},
    "mul": {"symbol": "<*>", "emit": "*"},
    "div": {"symbol": "</>", "emit": "/"},
    "mod": {"symbol": "<%>", "emit": "%"},
    "eq":  {"symbol": "<=>", "emit": "=="},
    "neq": {"symbol": "<!=>", "emit": "!="},
    "lt":  {"symbol": "<lt>", "emit": "<"},
    "gt":  {"symbol": "<gt>", "emit": ">"},
    "and": {"symbol": "<&&>", "emit": "&&"},
    "or":  {"symbol": "<||>", "emit": "||"},
    "not": {"symbol": "<!>", "emit": "!"}
  },
  "structure": {
    "action": "::",
    "range": "..",
    "colon": ":"
  },
  "syntax": {
    "emit": "emit",
    "grab": "grab",
    "val": "val",
    "with": "with"
  },
  "literals": {
    "numberPrefix": "#",
    "stringDelim": "\"",
    "comment": "@@"
  },
  "targets": {
    "py": {
      "name": "Python",
      "ext": ".py",
      "header": "#!/usr/bin/env python3\n# Transpiled from JibJab\n",
      "print": "print({expr})",
      "var": "{name} = {value}",
      "forRange": "for {var} in range({start}, {end}):",
      "forIn": "for {var} in {collection}:",
      "while": "while {condition}:",
      "if": "if {condition}:",
      "else": "else:",
      "func": "def {name}({params}):",
      "return": "return {value}",
      "call": "{name}({args})",
      "indent": "    ",
      "true": "True",
      "false": "False",
      "nil": "None",
      "and": "and",
      "or": "or",
      "not": "not "
    },
    "js": {
      "name": "JavaScript",
      "ext": ".js",
      "header": "// Transpiled from JibJab\n",
      "print": "console.log({expr});",
      "var": "let {name} = {value};",
      "forRange": "for (let {var} = {start}; {var} < {end}; {var}++) {",
      "forIn": "for (const {var} of {collection}) {",
      "while": "while ({condition}) {",
      "if": "if ({condition}) {",
      "else": "} else {",
      "func": "function {name}({params}) {",
      "return": "return {value};",
      "call": "{name}({args})",
      "blockEnd": "}",
      "indent": "    ",
      "true": "true",
      "false": "false",
      "nil": "null",
      "eq": "===",
      "neq": "!=="
    },
    "c": {
      "name": "C",
      "ext": ".c",
      "header": "// Transpiled from JibJab\n#include <stdio.h>\n#include <stdlib.h>\n",
      "printInt": "printf(\"%d\\n\", {expr});",
      "printStr": "printf(\"%s\\n\", {expr});",
      "var": "int {name} = {value};",
      "forRange": "for (int {var} = {start}; {var} < {end}; {var}++) {",
      "while": "while ({condition}) {",
      "if": "if ({condition}) {",
      "else": "} else {",
      "func": "int {name}({params}) {",
      "funcDecl": "int {name}({params});",
      "return": "return {value};",
      "call": "{name}({args})",
      "blockEnd": "}",
      "indent": "    ",
