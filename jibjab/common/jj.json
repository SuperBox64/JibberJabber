{
  "version": "1.0.0",
  "keywords": {
    "print": "~>frob{7a3}",
    "input": "~>slurp{9f2}",
    "yeet": "~>yeet",
    "snag": "~>snag",
    "invoke": "~>invoke",
    "nil": "~nil",
    "true": "~yep",
    "false": "~nope"
  },
  "blocks": {
    "loop": "<~loop{",
    "when": "<~when{",
    "else": "<~else>>",
    "morph": "<~morph{",
    "try": "<~try>>",
    "oops": "<~oops>>",
